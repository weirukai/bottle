@import "/pages/common/lib/weui.wxss";

.page-body-info {
  display: flex;
  margin-top: 40rpx;
  padding: 0;
  height: 360rpx;
  width: 70%;
  margin-left: 50px;
  border-top: 1rpx solid rgb(245, 253, 210);
  border-bottom: 1rpx solid rgb(245, 253, 210);
  align-items: center;
  justify-content: center;
  background-color: transparent;
}
.page-body-info image{
  width: 160rpx;
  height: 130rpx;
}
.page-section{
  height: 100%;
  width:100%;
  
}
.flex{
  display: flex;
  height: 10%;
}
.back{
  background-color: transparent;
}
.button{
font-family: "楷体";
border-radius: 20px;
box-shadow: 3px 3px 5px rgb(237, 238, 208)
}
.button-hover{
  background-color: rgb(149, 241, 241);
}
.button-hover1{
  background-color: rgb(149, 241, 241);
}
.container {
  display: flex;/*将对象作为弹性伸缩盒*/
  flex-direction: column;
  min-height: 100%;
  justify-content: space-between;
  font-size: 32rpx;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
  background-repeat: no-repeat;
  background-size: cover;
  background-image: url("data:image/jpeg;base64,")
  
}