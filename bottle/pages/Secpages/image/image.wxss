@import "../../common/lib/weui.wxss";
.image {
  width: 100%;
  height: 360rpx;
}
.page-body-info {
  display: flex;
  box-sizing: border-box;
  padding: 80rpx;
  height:400rpx;
  align-items: center;
  justify-content: center;
  background: transparent;
 
  color:#FFF;
}
button::after{
border:none;
}

.goodbutton {
 margin-top: 35px; 
 margin-right:10px;
 float:left;
  width: 40%;
  background: transparent; 
  color:#000;
  border-radius: 98rpx;
  background: bg_red;
  font-family: "楷体";
  font-weight: 700;
  box-shadow:-3px -3px 3px rgb(234,212,180);

}
.hover {
  background:  rgb(232,211, 184);
}

.goodbutton1 {
margin-top: 35px; 
 margin-left:10px;
 float:right;
  width: 40%;
 background: transparent;
  color:#000;
  border-radius: 98rpx;
  background: bg_red;
  font-family: "楷体";
  font-weight: 700;
  box-shadow:-3px -3px 3px rgb(234,212,180);

}
 
/* 按下变颜色 */
.hover {
